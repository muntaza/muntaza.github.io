
wget -c https://slackbuilds.org/slackbuilds/15.0/office/libreoffice.tar.gz

bash-5.1$ tar -xzvf libreoffice.tar.gz 

bash-5.1$ cd libreoffice

bash-5.1$ wget -c https://download.documentfoundation.org/libreoffice/stable/25.2.3/rpm/x86_64/LibreOffice_25.2.3_Linux_x86-64_rpm.tar.gz

bash-5.1$ sudo sh libreoffice.SlackBuild



